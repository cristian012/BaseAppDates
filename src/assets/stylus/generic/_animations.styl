@keyframes shake
    59%
        margin-left: 0

    60%, 80%
        margin-left: 2px

    70%, 90%
        margin-left: -2px

.scale-center
    animation scale-center 0.3s cubic-bezier(0.250, 0.460, 0.450, 0.940) both
        
@keyframes scale-center
    0%
        transform scale(0)
    100%
        transform scale(1)
        
@keyframes slide-x-left
    0%
        transform translateX(-100%)
    100%
        transform translateX(0)